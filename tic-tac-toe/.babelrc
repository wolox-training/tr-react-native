{
  "presets": [
    "react-app"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "~components": "./src/app/components",
        "~screens": "./src/app/screens",
        "~redux": "./src/redux",
        "~utils": "./src/utils",
        "~services": "./src/services",
        "~config": "./src/config"
      }
    }]
  ]
}
